apiVersion: v1
kind: Secret
metadata:
  name: mosquitto-secrets
stringData:
  # see https://mosquitto.org/documentation/authentication-methods/
  # mosquitto_passwd -c password_file mqtt
  password_file: ""
---
apiVersion: v1
kind: Secret
metadata:
  name: mqtt-exporter-secrets
data:
  # same as mosquitto-secrets
  # # echo -n '...' | base64
  MQTT2PROM_MQTT_USER: ""
  MQTT2PROM_MQTT_PASSWORD: ""